    "astro": "^5.1.5",
    "astro-analytics": "^2.7.0",
    "astro-compressor": "^1.0.0",
    "astro-devtool-breakpoints": "^0.3.0",
    "astro-favicons": "^3.1.4",
    "astro-font": "^0.1.81",
    "astro-healthcheck-ts": "^1.0.2",
    "astro-icon": "^1.1.5",
